		"GeneralBookingReferenceDef": {
			"type": "object",
			"properties": {
				"allocator": {
					"$ref": "definitions.json#/definitions/CompanyDef",
					"description": "RICS code - allocator managing the booking"
				},
				"bookingId": {
					"description": "Reservation reference number (element 34) in case of reservations and IRTs",
					"type": "string"
				},
				"departureDate": {
					"$ref": "definitions.json#/definitions/date",
					"description": "To be used with systems where the bookingId is not unique"
				},
				"issuingDate": {
					"$ref": "definitions.json#/definitions/date",
					"description": "issuing date (UTC). The issuing date must not match exactly in a search, its purpose here is to uniquely identify the ticket if the BookingId is repeated after some months or years."
				},
				"systemCode": {
					"description": "optional system code in case an allocator runs technically different systems for his tickets (e.g. during a migration phase).",
					"type": "string"
				},
				"train": {
					"description": "train number - To be used with systems where the bookingId is not unique",
					"type": "string"
				},
				"accountingId": {
					"description": "id of the ticket used in the accounting data UIC 301 in case it differs from the BookingId.",
					"type": "string"
				}
			},
			"required": [
				"allocator",
				"bookingId",
				"issuingDate"
			],
			"patternProperties": {
				"^P_": {
					"anyOf": [
						{
							"type": "object"
