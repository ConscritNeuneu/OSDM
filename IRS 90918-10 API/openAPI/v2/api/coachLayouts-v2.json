{
  	"openapi": "3.0.0",
  	"info": {
    	"version": "1.0.0",
		"title": "UIC 90918-10",
  		"description": "download coach layouts",
  		"contact": {
    		"name": "UIC",
    		"url": "https://www.uic.org",
    		"email": "uic@uic.org"
    	},
