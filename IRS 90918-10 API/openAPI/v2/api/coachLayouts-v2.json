  		}
 	},
	"paths": {
		"/{version}/layouts": {
  			"get": {
    			"description": "get coach layouts",
        		"operationId": "getCoachLayouts",
        		"parameters": [
         			{
            			"name": "version",
            			"in": "path",
            			"description": "schema version",
            			"required": true,
            			"schema": {
              				"type": "string"  
            			}
          			}
        		],    			
     			"responses": {
        			"200": {          
          				"description": "coach layouts",
         				 "content": {
            				"application/json": {
              					"schema": {
               						"$ref": "../schemas/osdm-coachlayouts-schema-v2.json#/definitions/LayoutsDef"   
           						}
    				        }
          				}
        			},
         			"default": {
           				 "description": "unexpected error",
          				 "schema": {
             			 	"$ref": "../schemas/osdm-common-schema-v2.json#/definitions/ErrorDef"
             			 }
            		}
     			}
   			}
  		}
	}
}