  "item": [
    {
      "name": "Fare Offers Without card",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Test scenario 1\", ()=> {\r",
              "    var jsonData = pm.response.json();\r",
              "\r",
              "    var tripOffers = jsonData.tripOffers;\r",
              "    pm.test(\"Only 1 tripOffer expected\", function (){\r",
              "        pm.expect(tripOffers.length).to.eql(1);\r",
              "    });\r",
              "\r",
              "    var offers = jsonData.tripOffers[0].offers;\r",
              "    pm.test(\"Only 1 offer expected\", function (){\r",
              "        pm.expect(offers.length).to.eql(1);\r",
              "    });\r",
              "\r",
              "    var fares = jsonData.tripOffers[0].offers[0].fares;\r",
              "    pm.test(\"Only 4 fares expected\", function (){\r",
              "        pm.expect(fares.length).to.eql(4);\r",
              "    });\r",
              "});"
            ],
            "type": "text/javascript"
          }
        }
